<body>
    <div class="container">
      <!-- Example row of columns -->
      <!-- <div class="row vertical-divider"> -->
        <div class="col-md-6">
	      <form class="form-signin">
	        <h2 class="form-signin-heading">Onboard a device</h2>
	        <label for="inputEmail" class="sr-only">Email address</label>
	        <input type="text" id="clientId" class="form-control" placeholder="Client Id" required autofocus>
	        <label for="inputPassword" class="sr-only">Password</label>
	        <input type="password" id="clientSecret" class="form-control" placeholder="Client Secret" required>
	        <button class="btn btn-lg btn-default btn-block" type="submit">Sign in</button>
	      </form>	
       </div>
      <!-- </div> -->

